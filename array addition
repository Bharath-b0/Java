class HelloWorld {
    public static void main(String[] args) {
        int a[]={2,3,4,5,6};
        int b[]=new int[5];
        int c[]=new int[5];
        for(int i=0;i<5;i++){
            b[i]=a[i];
            c[i]=a[i]+b[i];
        }
        for(int n:b){
            System.out.println(n);
        }
        System.out.println("Addition:");
        for(int n:c){
            System.out.println(n);
        }
    }
}
